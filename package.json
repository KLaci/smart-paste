{
  "name": "smart-paste",
  "displayName": "Smart File Paste",
  "description": "Auto-generate file names from your pasted text.",
  "version": "0.0.3",
  "publisher": "laszlokiss",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/KLaci/smart-paste"
  },
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "title": "Smart Paste",
      "properties": {
        "smartPaste.openAIKey": {
          "type": "string",
          "default": "",
          "description": "Smart Paste OpenAI key"
        }
      }
    },
    "commands": [
      {
        "command": "smartPaste.customPaste",
        "title": "Create File from Paste"
      }
    ],
    "keybindings": [
      {
        "command": "smartPaste.customPaste",
        "key": "ctrl+v",
        "when": "explorerViewletFocus"
      },
      {
        "command": "smartPaste.customPaste",
        "key": "cmd+v",
        "when": "explorerViewletFocus"
      }
    ]
  },
  "scripts": {
    "compile": "node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "vscode:prepublish": "npm run package",
    "package": "node esbuild.js --production"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.92.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "esbuild": "^0.23.1",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "imports": {
    "#customImport": "./src/customImport.mjs",
    "#vscode": "./src/vscode.mjs"
  },
  "dependencies": {
    "@ai-sdk/openai": "^0.0.44",
    "ai": "^3.3.5"
  }
}
